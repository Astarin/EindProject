<div class="jumbotron">
    <h1>@ViewBag.Title</h1>
</div>
<div>
    @{
        string errorMsg = "";
        if (ViewBag.ErrorMsg != null)
        {
            errorMsg = (string)ViewBag.ErrorMsg;
        }
    }
    <label class="label-danger">@errorMsg.ToString()</label>
    @{
        if (ViewBag.OkMsg != null)
        {
            string okMsg = (string)ViewBag.OkMsg;
            <label class="label-info">@okMsg.ToString()</label>
        }
    }
</div>



